package org.java.loop;

public class For1 {

	public static void main(String[] args) {
		System.out.println("for문");
		
		for(int i=0; i<10; i++) {
//			System.out.println(i);
//			System.out.print(i+" ");
			int num = i;
			if(num>10) {
				System.out.print(num+1);
				System.out.println(" , ");
			}
			//1~10까지 콘솔에 출력
			
//			int i2=i+1;
//			System.out.println(i2);
			
			
		}
		
		
	}
}
