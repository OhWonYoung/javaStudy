package org.java.varable;

public class VarEx1 {
	public static void main(String[] args) {
		
		System.out.println("변수, 데이터 타입");
		System.out.println("primitive type");
		
		
		int a; // 게이터 타입이 int(정수형 4바이트)형인 변수 a를 선언 했다.
		a=1000;// 변수  a를 1000으로 초기화 했다.
		// 변수 = 하나의 데이터를 주저장장치(ram)에 저장한다.
		
		int a2=100;//선언과 동시에 초기화  ---- 할당(메모리(저장공간)에 변수);
		
		
	}
	
}
