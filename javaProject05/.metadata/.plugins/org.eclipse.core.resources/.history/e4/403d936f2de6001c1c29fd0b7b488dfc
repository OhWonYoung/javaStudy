package org.java.varable;

public class VarEx1 {
	public static void main(String[] args) {
		
		System.out.println("변수");
		System.out.println("java primitive type");
		
		//자바의 기본변수
		byte b=10;
		short s=10;
		int i=10;
		long l=10L;
		
		float f=1F;
		double d=1.1;
		
		char ch='a';
		
		boolean bool=false;
		
		
		
	}
	
}
