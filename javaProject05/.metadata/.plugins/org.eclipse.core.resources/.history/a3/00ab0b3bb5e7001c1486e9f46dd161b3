public class IfEx5 {

	public static void main(String[] args) {
		System.out.println("Á¶°Ç¹®");
//		
//		int num=85;
//		System.out.println();
//		
		if(grade>=90) {
			if(grade>=95) {
				System.out.println("A+");
			}else {
				System.out.println("A");
			}
		}else if(grade>=80) {
			System.out.println("B");
		}else if(grade>=70) {
			System.out.println("C");
		}else if(grade>=60) {
			System.out.println("D");
		}else {
			System.out.println("F");
		}
		
		
		
		
	}
}
