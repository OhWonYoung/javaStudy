package org.java.loop;

public class While2 {

	public static void main(String[] args) {
		System.out.println("while");
		// 구구단 2~9eks
		int i=2;//1이면 i++이 아래로
		
		while(i<10) {
			System.out.println(i+"단 입니다.");

			int j=1;
			while(j<10) {
				System.out.println(i+" X "+j+" = "+(i*j));
				j++;
			}
			
			i++;
		
		}
	
	
	
	}
}
