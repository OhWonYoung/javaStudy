package org.java.varable;

public class VarEx1 {
	public static void main(String[] args) {
		
		System.out.println("변수");
		
		int a; // 게이터 타입이 int(정수형 4바이트)형인 변수 a를 선언 했다.
		a=1000;// 변수  a를 1000으로 초기화 했다.
		// 변수 = 하나의 데이터를 주저장장치(ram)에 저장한다.
		
		
		System.out.println("java primitive type");
		
		//자바의 기본변수
		byte b=10;
		short s=10;
		int i=10;
		long l=10L;
		
		float f=1.1F;
		double d=1.1;
		
		char ch='a';
		
		boolean bool=false;
		
		
		
	}
	
}
